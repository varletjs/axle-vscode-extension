{
  "name": "axle-vscode-extension",
  "private": true,
  "displayName": "axle-vscode-extension",
  "icon": "src/icon.png",
  "description": "axle extension for vscode",
  "publisher": "haoziqaq",
  "author": "ahqrt",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:vue",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.findClipboardURL",
        "title": "find clipboard url"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/javascript.json"
      }
    ],
    "keybindings": [
      {
        "command": "extension.findClipboardURL",
        "key": "ctrl+shift+v",
        "mac": "cmd+shift+v",
        "when": "editorTextFocus"
      }
    ]
  },
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/varletjs/axle-vscode-extension.git"
  },
  "bugs": {
    "url": "https://github.com/varletjs/axle-vscode-extension/issues"
  },
  "scripts": {
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "build": "vsce package",
    "publish": "yarn compile & yarn build & vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "14.x",
    "@types/vscode": "^1.56.0",
    "glob": "^7.1.6",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vsce": "^2.7.0",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.4.0"
  }
}